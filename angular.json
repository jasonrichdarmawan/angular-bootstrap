        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/myaccount/**/*.ts",
              "projects/myaccount/**/*.html"
            ]
          }
        }
      }
    },
